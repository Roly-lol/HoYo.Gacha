{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Genshin Gacha",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "clipboard": { "all": true },
      "dialog": { "all": true },
      "shell": { "all": true }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.lgou2w.genshin.gacha",
      "longDescription": "Manage and analyze your Genshin Impact gacha logs",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "Manage and analyze your Genshin Impact gacha logs",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 960,
        "resizable": true,
        "center": true,
        "title": "Genshin Gacha",
        "width": 1280
      }
    ]
  }
}
